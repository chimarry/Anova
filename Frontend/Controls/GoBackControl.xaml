<UserControl x:Class="ANOVA.Frontend.Controls.GoBackControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ANOVA.Frontend.Controls"
             mc:Ignorable="d"            >
    <Button x:Name="GoBackButton" HorizontalAlignment="Center" VerticalAlignment="Center" Width="104" Background="#FF1F262A" Click="GoBackButton_Click">
        <StackPanel x:Name="GoBackStackPanel" Height="24" Width="90" Orientation="Horizontal">
            <TextBlock x:Name="GoBackText" Width="88" Foreground="White" FontWeight="Bold" Height="20" Text="       Go back"/>
        </StackPanel>
    </Button>
</UserControl>
